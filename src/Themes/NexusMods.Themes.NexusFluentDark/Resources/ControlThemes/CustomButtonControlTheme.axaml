                        <controls:CustomButton Type="Secondary" Size="Small" Fill="Strong" VisibleIcon="Left" LeftIcon="{x:Static icons:IconValues.Nexus}" />
                        <controls:CustomButton Type="Secondary" IsEnabled="False"  Size="Small" Fill="Strong" VisibleIcon="Left" LeftIcon="{x:Static icons:IconValues.Nexus}" />
                    </StackPanel>
