            <StackPanel Orientation="Horizontal" Spacing="36">
                
                <StackPanel Spacing="4">
                    <Button IsEnabled="False">Test</Button>
                    <controls:CustomButton IsEnabled="False"/>
                    <controls:CustomButton>
                        <StackPanel>
                        <TextBlock>Test 1</TextBlock>
                            <StackPanel Orientation="Horizontal" Background="Chocolate">
                                <icons:UnifiedIcon Value="{x:Static icons:IconValues.Check}"></icons:UnifiedIcon>
                                <icons:UnifiedIcon Value="{x:Static icons:IconValues.Check}"></icons:UnifiedIcon>
                                <icons:UnifiedIcon Value="{x:Static icons:IconValues.Check}"></icons:UnifiedIcon>
                            </StackPanel>
                            <TextBlock Background="DarkRed">Test 2</TextBlock>
