                    <TextBlock Grid.Row="1" Grid.Column="0" Text="Medium" Theme="{StaticResource BodyMDNormalTheme}"/>
                    
                    <StackPanel Grid.Row="3" Grid.Column="0" Spacing="24">
                        <StackPanel Spacing="8">
                            <TextBlock Text="Fill: None" Theme="{StaticResource BodyMDNormalTheme}"  />
                            <controls:CustomButton IsEnabled="True" Text="Enabled" VisibleIcon="Left" LeftIcon="{x:Static icons:IconValues.Check}"/>
                            <controls:CustomButton IsEnabled="False" Text="Disabled" VisibleIcon="Left" LeftIcon="{x:Static icons:IconValues.Close}"/>
                            <controls:CustomButton VisibleIcon="Both" />
                            <controls:CustomButton />
                            <controls:CustomButton VisibleIcon="Left" Text="Something" HorizontalAlignment="Stretch" LeftIcon="{x:Static icons:IconValues.PlayArrow}"/>
                            <controls:CustomButton VisibleIcon="Both" ShowLabel="False"/>
                            <controls:CustomButton VisibleIcon="Left" ShowLabel="False"/>
                        </StackPanel>
                    </StackPanel>
                    
                    <TextBlock Grid.Row="1" Grid.Column="2" Text="Small" Theme="{StaticResource BodyMDNormalTheme}"/>
                    
                    <StackPanel Grid.Row="3" Grid.Column="2" Spacing="24">
                        <StackPanel Spacing="8">
                            <TextBlock Text="Fill: None" Theme="{StaticResource BodyMDNormalTheme}"  />
                            <controls:CustomButton Size="Small" />
                            <controls:CustomButton Size="Small" VisibleIcon="Left" />
                            <controls:CustomButton Size="Small"  IsEnabled="False" VisibleIcon="Right" />
                            <controls:CustomButton Size="Small"  IsEnabled="False" VisibleIcon="Both" />
                            <controls:CustomButton Size="Small" VisibleIcon="Both" ShowLabel="False"/>
                            <controls:CustomButton Size="Small" VisibleIcon="Left" ShowLabel="False"/>
                        </StackPanel>
                    </StackPanel>
                </Grid>
                
                <Grid ColumnDefinitions="*,12,*" RowDefinitions="32,Auto,10,Auto,Auto,Auto" ShowGridLines="False">
