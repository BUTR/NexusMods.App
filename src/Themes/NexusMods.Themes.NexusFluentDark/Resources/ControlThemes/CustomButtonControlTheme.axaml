                            <controls:CustomButton Type="Primary" Text="Primary" IsEnabled="False" VisibleIcon="Left" />
                            <controls:CustomButton Type="Primary" VisibleIcon="Right" />
                            <controls:CustomButton Type="Primary" VisibleIcon="Both" />
                        </StackPanel>
                    </StackPanel>
                    
                    <StackPanel Grid.Row="5" Grid.Column="0"  Spacing="8">
                        <TextBlock Text="Fill: Weak" Theme="{StaticResource BodyMDNormalTheme}"  />
                        <controls:CustomButton IsEnabled="True" Type="Primary" Fill="Weak" />
                        <controls:CustomButton IsEnabled="False" Type="Primary" Fill="Weak" />
                    </StackPanel>
                    
                    <StackPanel Grid.Row="3" Grid.Column="2" Spacing="24">
                        <StackPanel Spacing="8">
                            <TextBlock Text="Fill: None" Theme="{StaticResource BodyMDNormalTheme}"  />
                            <controls:CustomButton Type="Primary" Size="Small" />
                            <controls:CustomButton Type="Primary" Size="Small" VisibleIcon="Left" />
                            <controls:CustomButton Type="Primary" Size="Small" VisibleIcon="Right" />
                            <controls:CustomButton Type="Primary" Size="Small" VisibleIcon="Both" />
                        </StackPanel>  
                    </StackPanel>
                                      
                    <StackPanel Grid.Row="5" Grid.Column="2"  Spacing="8">
                        <TextBlock Text="Fill: Weak" Theme="{StaticResource BodyMDNormalTheme}"  />
                        <controls:CustomButton IsEnabled="True" Type="Primary"  Size="Small" Fill="Weak" VisibleIcon="Right" />
                        <controls:CustomButton IsEnabled="False" Type="Primary" Size="Small"  Fill="Weak" VisibleIcon="Right" />
                    </StackPanel>
